{
  "name": "redux-async-initial-state",
  "version": "0.2.0",
  "description": "It is simple redux middleware that helps you load redux initial state asynchronously",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run test && rimraf lib && npm run build",
    "test": "mocha --compilers js:babel-core/register --reporter spec --require ./test/helper.js test/*.js",
    "posttest": "npm run lint",
    "lint": "eslint src test",
     "postinstall": "postinstall-build lib \"npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KELiON/redux-async-initial-state.git"
  },
  "keywords": [
    "redux",
    "initial",
    "state",
    "async"
  ],
  "author": "KELiON <kelionweb@gmail.com> (http://asubbotin.ru)",
  "license": "MIT",
  "dependencies": {
    "postinstall-build": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  }
}
